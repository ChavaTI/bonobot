{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "../../server",
    "resolveJsonModule": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
